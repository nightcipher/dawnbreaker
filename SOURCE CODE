class Player(object):
    def __init__(self, name, gender, race, playerClass):
        self.name = name
        self.gender = gender
        self.race = race
        self.playerClass = playerClass
    def __str__(self):
      result = ""
      result += self.name + " the " + self.gender + " " + self.race + " " + self.playerClass
      return result
